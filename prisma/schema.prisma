generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Content {
  id             String   @id
  recipient_name String
  word_sent      String
  created_at     DateTime @default(now())
  updated_at     DateTime @updatedAt

  @@map("contents")
}
